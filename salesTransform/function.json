{
  "bindings": [
    {
      "name": "inputBlob",
      "type": "blobTrigger",
      "direction": "in",
      "path": "salescontainer/raw/{yyyy}/{MM}/{dd}/{name}",
      "connection": "AzureWebJobsStorage"
    },
    {
      "name": "outputBlob",
      "type": "blob",
      "direction": "out",
      "path": "salescontainer/processed/{yyyy}/{MM}/{dd}/{name}",
      "connection": "AzureWebJobsStorage"
    }
  ]
}
